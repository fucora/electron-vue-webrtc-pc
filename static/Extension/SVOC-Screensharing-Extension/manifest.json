{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "SVOC Screensharing Extension",
  "description": "Enables screen sharing with SVOC",
  "version": "1.0.4",
  "manifest_version": 2,
  "minimum_chrome_version": "34",
  "icons": {
  },
  "permissions": [
    "desktopCapture",
    "storage"
  ],
  "icons": {
    "16": "images/16.png",
    "48": "images/48.png",
    "64": "images/64.png",
    "128": "images/128.png"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [ {
    "js": [ "content.js" ],
    "run_at": "document_end",
    "matches": [ "https://*/*" ]
  }],
  "options_page": "options.html"
}
